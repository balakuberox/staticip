ifcfg-ens192
[kuberox@helper1 network-scripts]$ cat ifcfg-ens192
TYPE=Ethernet
PROXY_METHOD=none
BROWSER_ONLY=no
BOOTPROTO=static
DEFROUTE=yes
IPV4_FAILURE_FATAL=no
IPV6INIT=yes
IPV6_AUTOCONF=yes
IPV6_DEFROUTE=yes
IPV6_FAILURE_FATAL=no
IPV6_ADDR_GEN_MODE=eui64
NAME=ens192
UUID=afdb67c2-e20f-469f-b184-d6179441aa89
DEVICE=ens192
ONBOOT=yes
DOMAIN=ocp4.ocpztp.com
IPADDR=190.170.1.40
PREFIX=24
GATEWAY=190.170.1.1
DNS1=190.170.1.1
